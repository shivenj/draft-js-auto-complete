{
  "name": "draft-js-auto-complete",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.4.4",
    "prop-types": "15.7.2",
    "draft-js": "^0.10.5",
    "draft-js-multidecorators": "https://github.com/shivenj/draft-js-multidecorators"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "react-scripts start",
